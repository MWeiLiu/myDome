{
    "name": "我的始终",
    "version": "1.0",
    "manifest_version": 2,
    "description": "我的第一个Chrome扩展",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "icons": {
        "16": "images/16_16.png"
    },
    "permissions": [
        "tabs",
        "alarms",
        "bookmarks",
        "browsingData",
        "contentSettings",
        "contextMenus",
        "cookies",
        "http://*/*",
        "desktopCapture",
        "webRequest"
    ],
    "browser_action": {
        "default_icon": "images/icon.jpg",
        "default_popup": "popup.html"
    },
    "devtools_page": "pages/devtools.html"
}