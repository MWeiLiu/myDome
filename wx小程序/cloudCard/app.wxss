@import "weui.wxss";
@import "toast/toast.wxss";
@import "font/iconfont.wxss";
page{font-size: 14px;}
.overhiden{overflow: hidden;}
.fl{float: left;}
.fr{float: right;}
.show{display: block !important;}
.hide{display: none !important;}
.weui-media-box__title{font-size: 14px;}
.p_shade {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 20;
}
.p_tag {
  position: absolute;
  width: 90%;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  -webkit-transform: translate(-50%, -50%);
  -moz-transform: translate(-50%, -50%);
  -ms-transform: translate(-50%, -50%);
  z-index: 21;
  background: #fff;
  border-radius: 5px;
  text-align: center;
}
.i_swiper_list{
  margin-top: 10px;
}
.i_item{
  position: relative;
  object-fit: cover;
  margin-bottom: 10px;
  height: 250rpx;
  padding: 10px;
}
.i_listBG{
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background: rgba(0,0,0,0.6);
  z-index: 2
}
.i_item .i_list_img{
  display: none;
}
.i_list{
  position: relative;
  z-index: 3;
  color: #fff;
  height: 250rpx;
}
.i_recmend .i_list .i_list_rigth{
  position: absolute;
  right: 0;
  top: 0;
}
.i_recmend .i_list .i_list_cont{
  text-align: center;
  padding-top: 10%;
}
.i_recmend .i_list .i_list_header{
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 0;
  display: flex;
  align-items: center;
}
.i_recmend .i_list .i_list_header image{
  width: 30px;
  height: 30px;
  border-radius: 50%;
  margin-right: 10px;
}
.m_list_tag{
  overflow: hidden;
  margin: 3px 0;
}
.m_list_tag span{
  float: left;
  color: blue;
  margin-right: 10px;
}
.m_discuss{
  padding-bottom: 55px;
}
.m_discuss_tit{
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 10px;
  border-bottom: solid 1px #e5e5e5;
}
.m_discuss_tit span{
  font-size: 16px;
}
.m_list{
  padding: 10px;
  border-bottom: solid 1px #e5e5e5;
}
.m_list_top{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.m_list_top .m_list_name {
  display: flex;
  align-items: center;
}
.m_list_top .m_list_name image{
  width: 40px;
  height: 40px;
  border-radius: 50%;
  margin-right: 5px;
}
.m_list_top .m_list_star image{
  width: 20px;
  height: 20px;
}
.m_list_date{
  margin: 10px 0;
}
.m_list_date span{
  margin-right: 15px;
}
.m_list_desc{
  color: #999;
  font-size: 12px;
}
.m_list_img {
  overflow: hidden;
}
.m_list_img image{
  width: 60px;
  height: 60px;
  float: left;
  margin-right: 10px;
}
.m_list_reply{
  background: #eee;
  padding: 10px;
  margin-top: 10px;
}
.m_list_all{
  text-align: right;
  padding-right: 10px;
}
.m_foot{
  background: #d81e06;
  color: #fff;
  text-align: center;
  width: 100%;
  height: 50px;
  line-height: 50px;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 10;
}
.m_top_desc{
  background: #eee;
  border: solid 1px #e5e5e5;
  padding: 20px 10px;
  text-align: center;
  font-size: 16px;
}
.m_note{
  margin: 10px 0;
  border-bottom: solid 1px #e5e5e5;
}
.m_note .m_note_tit{
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 10px 0;
}
.m_note .m_note_tit a{
  display: block;
}
.m_note .m_note_tit a .icon-rank{
  font-size: 22px;
}
.m_note .m_note_desc view{
  font-size: 12px;
  margin-bottom: 10px;
  color: #999;
}
.home {
  position: absolute;
  width: 750rpx;
  height: 100%;
  background-color: white;
  z-index: 1;
  transition: All 0.4s ease;
  -webkit-transition: All 0.4s ease;
}
.home .icon_user {
  width: 68rpx;
  height: 68rpx;
  margin-left: 20rpx;
  margin-top: 20rpx;
}
.i_shade {
  display: none;
  position: fixed;
  top: 0;
  left: 70%;
  width: 100%;
  height: 100%;
  background-color: black;
  z-index: 1001;
  -moz-opacity: 0.7;
  opacity: 0.70;
  transition: width 2s;
  filter: alpha(opacity=70);
}
.page_tab {
  height: 100%;
  width: 70%;
  position: fixed;
  background-color: #fff;
  z-index: 0;
  overflow-y:scroll;
}
.i_head{
  height: 35px;
  line-height: 35px;
  padding: 0 10px;
  position: relative;
}
.i_head .icon-my,.i_head .icon-search{
  width: 40px;
  font-size: 20px;
  text-align: center;
}
.i_left_top{
  width: 100%;
  position: relative;
}
.i_left_top .i_left_sign{
  position: absolute;
  left: 5px;
  top: 5px;
  text-align: center;
}
.i_left_top .i_left_sign .icon-qiandao{
  font-size: 20px;
}
.i_left_top .i_left_head {
  text-align: center;
  padding-top: 5%;
}
.i_left_top .i_left_head image{
  width: 50px;
  height: 50px;
  border-radius: 50%;
}
.i_left_top .i_left_login{
  display: flex;
  align-items: center;
  justify-content: center;
}
.i_userList{
  display: flex;
  align-items: center;
  justify-content: space-between;
  text-align: center;
  padding: 0 10px;
  margin: 10px 0;
}
.i_left_list{
  padding: 0 10px;
}
.i_left_list .i_left_cont{
  display: flex;
  align-items: center;
  margin-bottom: 10px;
}
.i_left_list .i_left_cont span{
  margin-right: 5px;
}
.i_left_list .i_left_contact button{
  display: inline-block;
  background: initial;
  border-radius: 0;
  line-height: initial;
  padding: 0;
  font-size: 14px;
}
.i_left_list .i_left_contact button::after{
  border: none;
}
.i_left_list .i_left_contact span{
  margin-right: 5px;
}