.city_first_letter{
  line-height: 40rpx;
  height: 40rpx;
  padding-left: 25rpx;
  font-size: 28rpx;
  background: #eee;
  color: #999;
}
.city_name{
  display: block;
  line-height: 80rpx;
  height: 80rpx;
  border-bottom: 1px solid #efefef;
  font-size: 28rpx;
  color: #333;
  padding-left: 25rpx;
}
.s_anew{
  color: #33b9ff;
}
.s_tit{
  position:absolute;
  left:0px;
  top:0px;
  width:50px;
  height:30px;
  line-height:30px;
  text-align:center;
  border-right:solid 1px #e5e5e5;
  padding-left: 5px;
}
.s_tit span{
  font-size: 14px;
}
.s_tit icon{
  vertical-align: -2px;
}
.weui-icon-search_in-box{
  left: 65px;
}
.weui-search-bar__box{
  padding-left: 85px;
}
.s_more {
  padding: 10px;
}
.s_more .s_more_tit{
  margin-top: 10px;
}
.s_more .s_more_list{
  border-bottom: solid 1px #e5e5e5;
  padding: 10px 0;
}
.s_more .s_more_adr{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.s_more .s_more_desc{
  margin-top: 5px;
}