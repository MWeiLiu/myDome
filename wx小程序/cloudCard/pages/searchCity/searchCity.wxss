page{
  height: 100%;
}
.city_box{
  height:100%;
  background: #fff;
  display: flex;
  position: relative;
}
.city_left{
  flex: 1;
}
.city_right{
  width: 60rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  position: fixed; 
  right: 0;
  top: 50%;
  transform: translate(0,-50%);
  background: #fff;
  overflow-y: scroll;
}
.letter_item{
  flex: 1;
  display: block;
  font-size: 24rpx;
  color: #33B9FF;
  text-align: center;
}
.city_locate{
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-right: 10px;
}
.city_locate,.national{
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 1px solid #efefef;
  font-size: 28rpx;
  color: #333;
  padding-left: 10px;
}
.city_locate_title{
  color: #999;
  margin-right: 20rpx;
}
