
.iQuizzes {
    border: 1px solid #DDDDDD;
}
.iQuizzesBox {
    padding: 40rpx 0;
    position: relative; width: 100%; height: 100%; transform: translate(0%, 0px) translateZ(0px);
}

.typeName {
    width: 150rpx;
    height: 40rpx;
    margin: 0 auto 16rpx;
    font-size: 28rpx;
    color: #333333;
    letter-spacing: 1.86rpx;
}

.typeName image {
    width: 28rpx;
    height: 28rpx;
    margin-right: 8rpx;
    vertical-align: middle;
}

.question {
    padding: 0 32rpx;
    font-size: 32rpx;
    color: #333333;
    letter-spacing: 2.4rpx;
    line-height: 56rpx;
}

.answerItems {
    width: 264rpx;
    height: 98rpx;
    margin: 24rpx auto 40rpx;
    overflow: hidden;

}

.judgeAnswerItems {
    width: 88rpx;
    height: 92rpx;
    float: left;
    border: 2rpx solid #DDDDDD;
    border-radius: 50%;
    line-height: 88rpx;
    text-align: center;
    font-size: 28rpx;
    color: #333333;
    letter-spacing: 0;
}

.judgeAnswerItems:last-child {
    float: right;
}

.rightORwrong image{
    width: 44rpx;
    height: 44rpx;
    display: block;
    margin: 0 auto 40rpx;
}

.iQuizzesText {
    padding: 0 32rpx;
    font-size: 28rpx;
    color: #333333;
    letter-spacing: 2.8rpx;
    line-height: 48rpx;
}

.analysis {
    margin: 18rpx 0 40rpx;
}

.next {
    width: 192rpx;
    height: 72rpx;
    margin: 0 auto;
    background: #003989;
    border-radius: 8rpx;
    font-size: 28rpx;
    color: #FFFFFF;
    letter-spacing: 7.6rpx;
    line-height: 72rpx;
    text-align: center;
}