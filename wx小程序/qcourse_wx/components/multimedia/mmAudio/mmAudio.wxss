.audioOne{
  width:100%;
}
.audioTimeProgress{
  display: flex;
  display: -webkit-flex;
  justify-content: center;
  -webkit-justify-content:center;
  align-items: center;
  -webkit-align-items:center;
}
.audioProgress{
  width: 60%;
  margin: 0 10px;
}
.audioPlayPause{
  width: 45px;
  height: 45px;
  display: block;
  margin: 10px auto;
  cursor: pointer;
}