
/** 公共wxss **/
@import "../../utils/common/common.wxss";


/** 分页符 **/
@import "../../utils/pageBreaks/pbKeepon/pbKeepon.wxss";
@import "../../utils/pageBreaks/pbSplitter/pbSplitter.wxss";
@import "../../utils/pageBreaks/pbAward/pbAward.wxss";

@import "../../components/multimedia/mmAudio/mmAudio.wxss";


/* 优惠券 */
@import '../../utils/coupon/coupon.wxss';
@import '../../utils/operates/operateWrap.wxss';
page{
  height: 100%;
}
.container{
  width: 750rpx;
  height: 100%;
  overflow: hidden;
  position: relative; 
}
.pub_wrap{
  width: 750rpx;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background-color: rgba(0,0,0,.0);
  z-index: 2;
  overflow-y: auto;
}
.box {
    width: 750rpx;
    height: 100%;
    position: absolute;
    top: 0;
    left: 750rpx;
    color: #333;
    font-size: 16px;
    line-height: 36px;
    letter-spacing: 1.2px;
    z-index:1;
    background: #fff;
    overflow: hidden;
    overflow-y: auto;
}

.box > view {
    width: 100%;
}

.content > view {
    padding: 0 12px;
}

view{
  font-size: 16px;
  color: #333;
  letter-spacing: 2.6rpx;
}
.publicity_bottom{
  width: 100%;
  background-color:#f5f5f5;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
}
.p_bottom_pay{
  height: 104rpx;
}
.pub_b_share{
  width: 128rpx;
  height: 100%;
}
.pub_b_coupon{
  width: 108rpx;
  height: 100%;
}
.pub_b_img{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}
.pub_b_coupon .pub_b_img{
  margin-left: 4rpx;
}
.pub_b_study{
  flex:1;
  -webkit-flex:flex;
  height: 100%;
  background-color:#333;
  color: #fff;
  text-align: center;
  line-height: 104rpx;
}
.sharetip{
  color: #666;
  width: 100%;
  font-size: 24rpx;
}
.sharetip11{
  margin-bottom: 6rpx;
}
.sharetip_price{
  text-align:center;
  margin-bottom:20rpx;
  color:#D0021B;
  font-size:32rpx;
}
.sharetip_ico{
  width: 6rpx;
  height: 6rpx;
  background-color: #666;
  border-radius: 50%;
  margin-top: 10rpx;
  margin-right: 10rpx;
}
.sharetip2{
  margin: auto;
  width: 85%;
}
.sharetip_text{
  flex: 1;
  -webkit-flex:1;
}
/* 领取优惠券层,使用优惠券 */
.coupon_mask,.mycoupon_mask{
  position: fixed;
  top: 0;
  left:0;
  bottom:0;
  right:0;
  background-color:rgba(0,0,0,.42);
}
.coupon_wrap,.mycoupon_wrap{
  width: 100%;
  background-color: #fff;
}
.coupon_close,.mycoupon_close{
  height:108rpx;
  width:720rpx;
  line-height:108rpx;
  font-size:40rpx;
  text-align:right;
  margin:auto;
}
.receiveCoupon,.pub_couponpay{
  width: 100%;
  height: 102rpx;
  background-color:#333;
  color: #fff;
  font-size: 36rpx;
  letter-spacing: 12rpx;
  text-align: center;
  line-height: 102rpx;
  margin-top: 24rpx;
}
/* 分享 */
.share_wrap{
  width: 100%;
  height: 100%;
  background-color: #fff;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  position: fixed;
  z-index: 1;
}
.share_close{
  position: absolute;
  right: 20rpx;
  top: 10rpx;
  z-index: 1;
}
.canvasOne{
  width: 534rpx;
  height: 936rpx;
  background-color: #fff;
  /* background-image:url('https://www.qcourse.com/images/invitingCardBanner2.png');
  background-size: 100% 100%;
  background-position: center; */
  margin: auto;
  margin-top: 60rpx;
}
.share_qrcode{
   width: 248rpx;
  height: 248rpx; 
  position: absolute;
  z-index: 2;
}

/*侧栏课程列表*/
.side_wrap{
  width: 560rpx;
  height: 100%;
  background: #FFFFFF;
  box-shadow: 0 1px 4px 0 rgba(0,0,0,0.50);
  /* position: fixed;
  top: 0;
  left: 0; */
  overflow: hidden;
  overflow-y: auto;
  position: absolute;
  left: 190rpx;
}
.side_header{
  background: #5e5e5e;
  padding:56rpx 12rpx 56rpx 24rpx;
  width: 100%;
}
.side_hwrap{
  border-left:4rpx solid #fff;
  padding-left: 20rpx;
}
.side_htitle{
  color: #fff;
  font-size: 48rpx;
  line-height: 64rpx;
  font-style: PingFangSC-Semibold;
  text-align: justify;
}
.side_hsubtitle{
  color: #fff;
  font-size: 28rpx;
  line-height: 44rpx;
  letter-spacing: 3.4rpx;
  margin-top: 10rpx;
  text-align: justify;
}
.side_item{
  width: 100%;
  /* min-height: 88rpx; */
  border-bottom: 4rpx dashed #ddd;
}
.side_itemwrap{
  padding: 24rpx 6rpx 24rpx 24rpx;
}
.side_itemicon{
  width: 28rpx;
  height: 28rpx;
  border: 2rpx solid #333;
  margin-right: 8rpx;
  line-height: 32rpx; 
  font-size: 32rpx;
  /* text-align: center; */
}
.side_itemtitle{
  font-size: 24rpx;
}
.side_courselist{
  padding-left: 60rpx;
  overflow: hidden;
}
.side_citem{
  padding-right: 6rpx;
  height:80rpx;
  border-bottom: 4rpx dashed #ddd;
  font-size: 24rpx;
  line-height: 80rpx;
}
.side_citem:nth-last-child(1){
  border-bottom: none;
}


.sidetab_target{
  width: 88rpx;
  height: 88rpx;
  line-height: 88rpx;
  color: #fff;
  text-align: center;
  position: fixed;
  bottom: 40rpx;
  right: 40rpx;
  border-radius: 100%;
  border: 2rpx solid #333;
  background-color: rgba(0,0,0,0.70);
  z-index: 3;
}


.box_mask{
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  position: fixed;
  background-color: (0,0,0,0);
  z-index: 1;
}
@media screen and (max-width:374px){
  .canvasOne{
    width: 220px;
    height: 385px;
  }
}
@media screen and (min-width:414px){
  .canvasOne{
    width: 300px;
    height: 525px;
  }
}
