
<view class="header" style="background: {{background}}">
    <view class="shade1" style="{{json.wholeSet.userLogo == '' ? 'background-color: rgba(0, 0, 0, 0);' : 'background-color: rgba(123, 123, 123, 0.3);'}}">
        <view class="changeBox">
            <view class="changeInfoBoxs" style="color: {{color}};">
                <view class="panme">{{pname}}</view>
                <view class="courseSubtitle">{{json.wholeSet.subTitle}}</view>
                <view class="userIcoBox">
                    <image src="{{json.wholeSet.header.headerList[json.wholeSet.header.headerON].userImg}}"></image>
                    <text>{{json.wholeSet.header.headerList[json.wholeSet.header.headerON].userName}}</text>
                </view>
            </view>
        </view>
    </view>
</view>