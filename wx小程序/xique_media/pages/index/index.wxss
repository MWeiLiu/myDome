/**index.wxss**/

@import "../../utils/common/common.wxss";

.swiper {
    height: 245px;
}

.horizontal {
    height: 95px;
}

.horizontal .horizontalItem {
    width: 25%;
    display: inline-block;
    margin-top: 20px;
}

.horizontal image {
    width: 40px;
    height: 33px;
}

.horizontal {
    font-size: 12px;
    color: #353535;
    text-align: center;
    line-height: 17px;
}

.weddingBox {
    height:145px;
    padding: 16px 12px;
    position: relative;
}

.weddingBox image {
    width: 82px;
    height: 68px;
    position: absolute;
}

.weddingBox image:nth-child(1) {
    width:174px;
    height:145px;
    left: 12px;
    top: 16px;
}

.weddingBox image:nth-child(2) {
    left: 192px;
    top: 18px;
}

.weddingBox image:nth-child(3) {
    left: 280px;
    top: 18px;
}

.weddingBox image:nth-child(4) {
    left: 192px;
    top: 88px;
}

.weddingBox image:nth-child(5) {
    left: 280px;
    top: 88px;
}

.aboutUs {
    margin: 0px;
}

.aboutUsBox image {
    width: 100%;
    height: 150px;
    margin-top: 16px;
}

.aboutUsBox {
    font-size: 13px;
    color: rgb(75, 75, 75);
    line-height: 25px;
    text-align: justify;
}

.aboutUsBox view {
    padding: 0 20px;
}